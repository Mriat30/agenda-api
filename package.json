{
  "name": "agenda-masajes",
  "version": "1.0.0",
  "description": "Skeleton for new typescript services based on hexagonal architecture",
  "main": "src/index.js",
  "scripts": {
  "start": "node dist/src/main.js",
  "prisma:generate": "prisma generate",
  "prisma:push": "prisma db push",
  "start:dev": "npm run prisma:generate && tsnd --respawn --poll src/main.ts",
  "dev": "npm run start:dev",
  "test": "npm run test:unit",
  "test:unit": "npm run prisma:generate && NODE_ENV=test jest --coverage",
  "test:integration": "npm run prisma:generate && npm run prisma:push && NODE_ENV=test jest tests/users/infrastructure/user-repository/prisma-user-repository.test.ts --runInBand",
  "test:docker": "npm run prisma:generate && npm run prisma:push && NODE_ENV=test jest --runInBand",
  "prepare": "husky install",
  "lint": "eslint --ignore-path .eslintignore . --ext .js,.ts",
  "lint:fix": "npm run lint -- --fix",
  "build": "npm run build:clean && npm run prisma:generate && npm run build:tsc",
  "build:clean": "rimraf dist; exit 0",
  "build:tsc": "tsc -p tsconfig.prod.json",
  "test:bdd": "NODE_ENV=test cucumber-js --tags 'not @wip'",
  "test:all": "npm run lint && npm run test:unit && npm run test:bdd --runInBand",
  "swagger": "node ./swagger.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertHernandez/hexagonal-architecture-typescript-service-skeleton.git"
  },
  "keywords": [
    "typescript",
    "hexagonal architecture",
    "skeleton"
  ],
  "author": "alberthernandezdev@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.3.0"
  },
  "bugs": {
    "url": "https://github.com/AlbertHernandez/hexagonal-architecture-typescript-service-skeleton/issues"
  },
  "homepage": "https://github.com/AlbertHernandez/hexagonal-architecture-typescript-service-skeleton#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@cucumber/cucumber": "^12.4.0",
    "@types/chai": "^5.2.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "gherkin-lint": "^4.2.4",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-gherkin": "^3.1.3",
    "prisma": "^5.22.0",
    "rimraf": "^4.0.4",
    "supertest": "^7.1.4",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.(js|ts)": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "body-parser": "^1.20.1",
    "chai": "^6.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
